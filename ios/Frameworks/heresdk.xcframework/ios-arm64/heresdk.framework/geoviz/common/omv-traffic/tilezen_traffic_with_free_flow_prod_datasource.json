{
    "datasources": {
        "omv" : {
            "provider": {
                "name": "tilezen_traffic_flow",
                "server": "https://traffic.vector.hereapi.com/v2/traffictiles/flow/mc/{z}/{x}/{y}/omv",
                "cache": {
                    "path": "cache/tilezen_traffic_flow"
                },
                "tiling": "QuadTreeMercator",
                "levels": {
                    "min_storage_level": 2,
                    "max_storage_level": 15
                }
            },
            "layers": [
                {
                    "name": "tilezen_traffic_flow.line",
                    "geometry": "line",
                    "render_as": "shiftable"
                }
            ]
        }
    }
}
